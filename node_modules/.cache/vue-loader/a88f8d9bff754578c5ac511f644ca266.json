\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n.phoneForm {\n  margin: 0 auto;\n  justify-content: center;\n  margin-bottom: 20px;\n}\n.phoneInput {\n  height: 43px;\n  margin-right: 11px;\n}\n.v-tour__target--highlighted {\n  box-shadow: 0 0 0 99999px rgba(0, 0, 0, 0.4) !important;\n}\n",{"version":3,"sources":["Index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwnBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Index.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div>\n    <div v-if=\"loading\" class=\"my-5 text-center loadinghh\">\n      <img height=\"40\" src=\"img/icons/loader.svg\" />\n      <p class=\"mt-2 small\">Loading Products</p>\n    </div>\n    <div v-if=\"!loading\" class=\"wrapper\">\n      <div v-if=\"noMerchant\">\n        <div class=\"container text-center\">\n          <img src=\"img/icons/not_found.svg\" height=\"80\" class=\"mb-3 mt-4\" />\n          <p>Merchant not found with provided URL!</p>\n          <a href=\"/\" class=\"btn btn-white text-capitalize mt-2 btn-sm\">\n            <span class=\"fa fa-home mr-2 p-color\"></span>\n            Visit Homepage\n          </a>\n        </div>\n      </div>\n\n      <div v-if=\"networkFail\">\n        <div class=\"container text-center\">\n          <img src=\"img/icons/emptystr.svg\" height=\"80\" class=\"mb-3 mt-4\" />\n          <p>Unable to Load Products!</p>\n          <button @click=\"reloadPage\" class=\"btn btn-white text-capitalize mt-2 btn-sm\">Try again</button>\n        </div>\n      </div>\n      <div v-else>\n        <div v-if=\"noResult\">\n          <div class=\"container\">\n            <div class=\"row\">\n              <div class=\"col-md-5 mx-auto\">\n                <div class=\"text-center my-5\">\n                  <img src=\"img/icons/not_found.svg\" height=\"150\" class=\"mb-3 mt-4\" />\n                  <p>\n                    Oops, it looks like we donâ€™t have a product named\n                    <span\n                      class=\"font-weight-bold\"\n                    >\"{{ searched }}\"</span>\n                    in our database yet. Kindly, leave your phone number and we ll find it and call you back for delivery within 30 mins\n                  </p>\n                  <div class=\"text-center mx-auto\">\n                    <form @submit.prevent=\"submitConcierge\" class=\"form-inline phoneForm\">\n                      <div class=\"form-group\">\n                        <input\n                          required\n                          v-model=\"phone\"\n                          type=\"text\"\n                          class=\"form-control phoneInput\"\n                          placeholder=\"Phone Number\"\n                        />\n                        <button :disabled=\"sendingConc\" class=\"btn text-capitalize btn-custom\">\n                          <img v-if=\"sendingConc\" height=\"18\" src=\"img/icons/loader.svg\" />\n                          <span v-else>\n                            <span class=\"fa fa-check mr-2\"></span>Submit\n                          </span>\n                        </button>\n                      </div>\n                    </form>\n                  </div>\n                  <button\n                    @click=\"viewAllProduct\"\n                    class=\"btn btn-white text-capitalize mt-2 btn-sm\"\n                  >View all Products</button>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div v-else>\n          <div class=\"container\">\n            <div class>\n              <div class=\"filter\">\n                <div v-if=\"vendor.available && !noMerchant\" class=\"row mx-lg-0 my-auto\">\n                  <div class=\"col-12\">\n                    <p class=\"font-weight-bold\">\n                      <span>\n                        Welcome to\n                        <span class=\"text-capitalize\">{{ vendor.name }}</span>'s Store\n                      </span>\n                    </p>\n                  </div>\n                </div>\n                <div class=\"row mx-lg-0 my-auto\" v-if=\"!noMerchant\">\n                  <div class=\"col-7\">\n                    <span class=\"my-auto pl-lg-0 text-white\">Filter by Categories</span>\n                  </div>\n                  <div class=\"col-5 text-right\">\n                    <button\n                      @click=\"clearFilterC\"\n                      v-if=\"clearFilterCat\"\n                      class=\"btn btn-white btn-sm text-capitalize\"\n                    >\n                      <span class=\"fa fa-cut mr-2 p-color\"></span> Clear Filter\n                    </button>\n                  </div>\n                </div>\n                <div id=\"filterCategories\" class=\"filteritem mt-3\">\n                  <a\n                    @click=\"filterProduct(category.name)\"\n                    :class=\"{ itemselected: itemselected == category.name }\"\n                    href=\"javascipt:void(0)\"\n                    v-for=\"(category, index) in categories\"\n                    :key=\"index\"\n                  >{{ category.name }}</a>\n                </div>\n                <p\n                  class=\"small mt-3 mb-3 text-right d-lg-none\"\n                  v-if=\"!noMerchant\"\n                >Scroll left for more Categories</p>\n                <div class=\"searchrel mt-3 mb-1\" id=\"searchfield\" v-if=\"!noResult && !noMerchant\">\n                  <div class>\n                    <form action method=\"GET\">\n                      <div class=\"searchbox\">\n                        <form @submit.prevent=\"searchProduct\">\n                          <div class=\"row\">\n                            <div class=\"col-7 col-md-10 pr-0\">\n                              <input\n                                v-model=\"search\"\n                                type=\"text\"\n                                placeholder=\"Search for a product\"\n                                class=\"form-control searchform\"\n                              />\n                            </div>\n                            <div class=\"col-5 col-md-2\">\n                              <button\n                                type=\"submit\"\n                                class=\"btn btn-white btn-block text-capitalize text-black searchbtn\"\n                              >\n                                <span class=\"fa fa-search mr-2\"></span>\n                                Search\n                              </button>\n                            </div>\n                          </div>\n                        </form>\n                      </div>\n                    </form>\n                  </div>\n                </div>\n              </div>\n              <div class=\"my-lg-4 my-2\"></div>\n            </div>\n            <div class=\"searchitem\">\n              <div v-if=\"loadingCat\" class=\"my-5 text-center\">\n                <img height=\"40\" src=\"img/icons/loader.svg\" />\n              </div>\n              <div v-else>\n                <div class=\"my-2\" v-if=\"searchInfoo\">\n                  <div class=\"row\">\n                    <div class=\"col-6\">\n                      <p class>\n                        Your search result -\n                        <span\n                          class=\"p-color\"\n                        >{{ productDyn.length }} items found</span>\n                      </p>\n                    </div>\n                    <div class=\"col-6 text-right pr-3\">\n                      <button @click=\"clearSearch\" class=\"btn btn-white btn-sm text-capitalize\">\n                        <span class=\"fa fa-cut mr-2 p-color\"></span> Clear\n                        Filter\n                      </button>\n                    </div>\n                  </div>\n                </div>\n\n                <div class=\"row pr-lg-4 pr-3\">\n                  <div\n                    class=\"col-md-3 pb-lg-3 pr-0 itemcol\"\n                    v-for=\"(product, index) in productDyn\"\n                    :key=\"index\"\n                  >\n                    <div class=\"card itemcard\">\n                      <div class=\"card-body\">\n                        <p class=\"itemname mb-0\">{{ product.product_name }}</p>\n                        <p class=\"itemprice\">{{ product.actual_price | currency('RwF', 0) }}</p>\n                        <div class=\"mt-2\">\n                          <div class=\"row\">\n                            <div class=\"col\">\n                              <span class=\"itemqua\">{{ product.category }}</span>\n                            </div>\n                            <div class=\"col-auto\">\n                              <p class=\"text-right m-0\">\n                                <span class=\"itemqua text-capitalize\">{{ product.unit }}</span>\n                              </p>\n                            </div>\n                          </div>\n\n                          <div class=\"mt-4\">\n                            <div v-if=\"!product.addingCart\" class>\n                              <p class=\"m-0 inCart\" v-if=\"checkInCart(product.uuid)\">In Cart</p>\n                              <p class=\"m-0 outStock\" v-else-if=\"product.quantity < 1\">Out of Stock</p>\n                              <button\n                                id=\"addtoCartbtn\"\n                                v-else\n                                v-show=\"toggleInput !== product.id\"\n                                @click=\"showInput(product, index)\"\n                                class=\"btn cartbtn btn-btn-custb btn-custom addToCart btn-block\"\n                              >\n                                <span class=\"fa fa-cart-plus mr-1\"></span>\n                                Add to Cart\n                              </button>\n                            </div>\n\n                            <div v-if=\"toggleInput === product.id\" class=\"inputQuantity\">\n                              <div class=\"quantty\">\n                                <button\n                                  @click=\"incrementQuant(index, product)\"\n                                  class=\"btn cartbtn btn-custb addToCart minbtn\"\n                                >\n                                  <span class=\"fa fa-plus\"></span>\n                                </button>\n\n                                <input\n                                  type=\"number\"\n                                  v-model=\"productQuantity[index].qty\"\n                                  min=\"1\"\n                                  :max=\"product.quantity\"\n                                  placeholder=\"Quantity\"\n                                  class=\"form-control text-center font-weight-bold\"\n                                />\n\n                                <button\n                                  @click=\"decrementQuant(index, product)\"\n                                  class=\"btn cartbtn btn-custb addToCart minbtn\"\n                                >\n                                  <span class=\"fa fa-minus\"></span>\n                                </button>\n                                <button\n                                  @click=\"\n                                    addProductCartFunc(\n                                      product,\n                                      productQuantity[index].qty\n                                    )\n                                  \"\n                                  class=\"btn cartbtn btn-custom addToCart ml-4 minbtn\"\n                                >\n                                  <span class=\"fa fa-check\"></span>\n                                </button>\n                              </div>\n                              <p class=\"text-center mb-2 mt-3\">\n                                <a @click=\"hideInput\" class=\"text-coll\">Cancel</a>\n                              </p>\n                            </div>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div></div>\n\n    <v-tour name=\"myTour\" :steps=\"steps\" :options=\"myOptions\"></v-tour>\n  </div>\n</template>\n<script>\nimport VueNotifications from \"vue-notifications\";\n\nlet qtyy = {\n  qty: 1\n};\nexport default {\n  data() {\n    return {\n      //addingCart: false,\n      productQuantity: [],\n      vendor: {\n        available: false,\n        name: \"\"\n      },\n      noMerchant: false,\n      loading: false,\n      loadingCat: false,\n      itemselected: \"\",\n      sendingConc: false,\n      search: \"\",\n      networkFail: false,\n      searched: \"\",\n      sendConcierge: false,\n      noResult: false,\n      phone: \"\",\n      searchInfoo: false,\n      toggleInput: null,\n      clearFilterCat: false,\n      myOptions: {\n        useKeyboardNavigation: false,\n        labels: {\n          buttonSkip: \"Skip tour\",\n          buttonPrevious: \"Previous\",\n          buttonNext: \"Next\",\n          buttonStop: \"Finish\"\n        }\n      },\n      steps: [\n        {\n          target: \"#filterCategories\", // We're using document.querySelector() under the hood\n          content: `<p class=\"small\">Scroll left for more Categories!</span>`,\n          params: {\n            placement: \"top\"\n          }\n        },\n        {\n          target: \"#addtoCartbtn\", // We're using document.querySelector() under the hood\n          content: `<p class=\"small\">Click to add product to Cart!</p>`,\n          params: {\n            placement: \"top\"\n          }\n        },\n        {\n          target: \"#searchfield\", // We're using document.querySelector() under the hood\n          content: `<p class=\"small\">Search for Products here!</p>`,\n          params: {\n            placement: \"top\"\n          }\n        }\n      ]\n    };\n  },\n  notifications: {\n    showLoginMsg: {\n      title: \"\",\n      message: \"\",\n      type: \"\"\n    }\n  },\n  computed: {\n    products() {\n      return this.$store.getters.availableProducts;\n    },\n    cartProducts() {\n      return this.$store.getters.cartProducts;\n    },\n    productDyn() {\n      /* this.products.forEach((product, index) => {\n        this.productQuantity.push(1);\n      }); */\n      const productdynamic = [];\n      this.products.forEach(element => {\n        productdynamic.push({\n          ...element,\n          addingCart: false,\n          itemAdded: false\n        });\n      });\n      // console.log(productdynamic);\n      return productdynamic;\n    },\n    categories() {\n      return this.$store.getters.availableCategories;\n    }\n  },\n  mounted() {\n    /* this.products.forEach((product, index) => {\n      this.productQuantity.push(1);\n    }); */\n    // console.log(this.productDyn);\n    this.$swal({\n      icon: \"info\",\n      showCancelButton: true,\n      cancelButtonColor: \"#000000\",\n      allowOutsideClick: false,\n      confirmButtonText: \"Yes, I agree\",\n      cancelButtonText: \"Not of age\",\n      text:\n        \"We sell alcoholic drinks to only people of 18+ of age. Failure to show your ID upon the delivery of your order, we will cancel it\",\n      confirmButtonColor: \"#e75563\"\n    }).then(result => {\n      if (result.value) {\n      } else {\n        window.location.href = \"https://google.com\";\n        console.log(\"Cancelled\");\n      }\n    });\n  },\n  created() {\n    this.loading = true;\n    if (this.$route.params.pathMatch) {\n      this.vendor.name = this.$route.params.pathMatch.substring(1);\n      this.vendor.available = true;\n      this.getVendorProduct(this.vendor.name);\n    } else {\n      this.getProductss();\n    }\n  },\n  methods: {\n    incrementQuant(index, product) {\n      const quantity = this.productQuantity[index].qty;\n      if (quantity < product.quantity) {\n        this.productQuantity[index].qty++;\n        this.productQuantity[index].qty = this.productQuantity[index].qty++;\n      }\n\n      //this.productQuantity[index] = this.productQuantity[index] + 1;\n      //console.log(this.productQuantity[index]);\n    },\n    decrementQuant(index, product) {\n      const quantity = this.productQuantity[index].qty;\n      if (quantity > 1) {\n        this.productQuantity[index].qty--;\n      }\n      //this.productQuantity[index] = this.productQuantity[index] - 1;\n      //console.log(this.productQuantity[index]);\n    },\n    getVendorProduct(vendor) {\n      this.$store\n        .dispatch(\"fetchVendorProductProducts\", vendor)\n        .then(resp => {\n          this.loading = false;\n          let nArray = [];\n          for (var i = 0; i < resp.length; i++) {\n            nArray[i] = {\n              qty: 1\n            };\n          }\n          this.productQuantity = nArray;\n          if (!localStorage.getItem(\"barttyUser\")) {\n            localStorage.setItem(\"barttyUser\", true);\n            this.$tours[\"myTour\"].start();\n          }\n        })\n        .catch(err => {\n          if (err == 404) {\n            console.log(\n              \"Merchant not found with provided URL or no products exist\"\n            );\n            this.loading = false;\n            this.noMerchant = true;\n          }\n          this.loading = false;\n        });\n    },\n    getProductss() {\n      this.$store\n        .dispatch(\"fetchProducts\")\n        .then(resp => {\n          this.loading = false;\n          let nArray = [];\n          for (var i = 0; i < resp.length; i++) {\n            nArray[i] = {\n              qty: 1\n            };\n          }\n          this.productQuantity = nArray;\n          if (!localStorage.getItem(\"barttyUser\")) {\n            localStorage.setItem(\"barttyUser\", true);\n            this.$tours[\"myTour\"].start();\n          }\n        })\n        .catch(err => {\n          this.loading = false;\n          this.networkFail = true;\n        });\n    },\n    showNotification(title, name, MsgType) {\n      this.showLoginMsg({\n        title: title,\n        message: name,\n        type: MsgType,\n        timeout: 4000\n      });\n    },\n    submitConcierge() {\n      let phone = this.phone;\n      let search = this.search;\n\n      this.sendingConc = true;\n      this.$store\n        .dispatch(\"concierge\", { phone, search })\n        .then(resp => {\n          this.phone = \"\";\n          this.sendingConc = false;\n          this.showNotification(\n            \"Sent Successfully\",\n            resp.data.message,\n            \"success\"\n          );\n        })\n        .catch(err => {\n          this.showNotification(\n            \"Sending failed!\",\n            \"Unable to send phone number now, please try later.\",\n            \"error\"\n          );\n          this.sendingConc = false;\n        });\n    },\n    showInput(product, index) {\n      // this.productDyn[index];\n      //product.addingCart = true;\n      if (this.productDyn[index].id === product.id) {\n        this.toggleInput = product.id;\n      }\n      this.productQuantity.push({ qty: 1 });\n      //console.log((this.productDyn[index].addingCart = true));\n      //console.log(product.addingCart);\n      //console.log(product);\n    },\n    hideInput() {\n      if (this.toggleInput) {\n        this.toggleInput = null;\n      }\n    },\n    checkInCart(productId) {\n      const cart = this.cartProducts;\n      const isAvailable = cart.find(item => item.product.uuid === productId);\n      if (isAvailable) {\n        // console.log(\"In Cart\");\n        return true;\n      } else {\n        // console.log(\"Not in Cart\");\n        return false;\n      }\n    },\n    viewAllProduct() {\n      this.search = \"\";\n      this.noResult = false;\n      this.$store\n        .dispatch(\"searchProduct\", this.search)\n        .then(resp => {\n          this.loading = false;\n          this.noResult = false;\n        })\n        .catch(err => {\n          if (err == 404) {\n            this.searched = this.search;\n            this.loading = false;\n            this.noResult = true;\n          }\n        });\n    },\n    clearSearch() {\n      this.searchInfoo = false;\n      this.viewAllProduct();\n    },\n    searchProduct() {\n      this.itemselected = \"\";\n      this.clearFilterCat = false;\n      this.searchInfoo = true;\n      let search = this.search;\n      this.loading = true;\n\n      if (this.$route.params.pathMatch) {\n        let VendorName = this.$route.params.pathMatch.substring(1);\n        this.$store\n          .dispatch(\"searchVendorProduct\", { VendorName, search })\n          .then(resp => {\n            this.loading = false;\n            this.noResult = false;\n          })\n          .catch(err => {\n            if (err == 404) {\n              this.searched = this.search;\n              this.loading = false;\n              this.noResult = true;\n            }\n          });\n      } else {\n        this.$store\n          .dispatch(\"searchProduct\", search)\n          .then(resp => {\n            this.loading = false;\n            this.noResult = false;\n          })\n          .catch(err => {\n            if (err == 404) {\n              this.searched = this.search;\n              this.loading = false;\n              this.noResult = true;\n            }\n          });\n      }\n    },\n    filterProduct(category) {\n      this.itemselected = category;\n      this.clearFilterCat = true;\n      this.loadingCat = true;\n\n      if (this.$route.params.pathMatch) {\n        let VendorName = this.$route.params.pathMatch.substring(1);\n        this.$store\n          .dispatch(\"filterVendorProduct\", { category, VendorName })\n          .then(resp => {\n            this.loadingCat = false;\n            this.noResult = false;\n          })\n          .catch(err => {\n            if (err == 404) {\n              this.searched = this.search;\n              this.loadingCat = false;\n              this.noResult = true;\n            }\n          });\n      } else {\n        this.$store\n          .dispatch(\"filterProduct\", category)\n          .then(resp => {\n            this.loadingCat = false;\n            this.noResult = false;\n          })\n          .catch(err => {\n            if (err == 404) {\n              this.searched = this.search;\n              this.loadingCat = false;\n              this.noResult = true;\n            }\n          });\n      }\n    },\n    reloadPage() {\n      location.reload();\n    },\n    addProductCartFunc(product, quantity) {\n      let productWithQuantity = {\n        product: product,\n        quantity: quantity\n      };\n      this.$store.dispatch(\"addProductToCart\", productWithQuantity);\n      this.hideInput();\n    },\n    clearFilterC() {\n      this.clearFilterCat = false;\n      this.itemselected = \"\";\n      this.viewAllProduct();\n    }\n  }\n};\n</script>\n<style>\n.phoneForm {\n  margin: 0 auto;\n  justify-content: center;\n  margin-bottom: 20px;\n}\n.phoneInput {\n  height: 43px;\n  margin-right: 11px;\n}\n.v-tour__target--highlighted {\n  box-shadow: 0 0 0 99999px rgba(0, 0, 0, 0.4) !important;\n}\n</style>\n"]}]}